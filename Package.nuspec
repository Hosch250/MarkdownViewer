      <dependency id="Microsoft.Toolkit.Parsers" version="5.0.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="MarkdownViewer\bin\Release\MarkdownViewer.dll" target="lib\NET472\MarkdownViewer.dll" />
  </files>
</package>