<?xml version="1.0"?>
<package >
  <metadata>
    <id>MarkdownViewer</id>
    <version>1.1.1</version>
    <authors>Hosch250</authors>
    <owners>Hosch250</owners>
    <projectUrl>https://github.com/Hosch250/MarkdownViewer</projectUrl>
    <license type="expression">Unlicense</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Supports markdown as parsed by Microsoft.Toolkit.Parsers. Includes support for the following markdown blocks:

Headers
Lists
Code
Quotes
Horizontal lines
Tables

And the following markdown inlines:

Bold
Italic
Strikethrough
Links
Images
Subscript
Superscript</description>
    <releaseNotes>Added Support for .NetCore</releaseNotes>
    <copyright>Copyright 2018</copyright>
    <tags>WPF Markdown</tags>
    <dependencies>
      <dependency id="AvalonEdit" version="6.0.1" />
      <dependency id="Microsoft.Toolkit" version="6.0.0" />
      <dependency id="Microsoft.Toolkit.Parsers" version="6.0.0" />
    </dependencies>
  </metadata>
